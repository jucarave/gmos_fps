            <kind>1</kind>
            <string>// Movement with keyboard...

var dir = scr_get_input_direction();

if (dir != -1){
    var spd = 3;
    if (state == SM_CROUCH){ spd = 1.5; }else
    if (state == SM_RUN){ spd = 5; }
    
    var xTo = cos(degtorad(dir));
    var yTo = sin(degtorad(dir));
    
    var size = GRID_HW - 2;
    
    var ofx = size;
    var ofy = size;
    
    if (xTo &lt; 0) ofx = -size;
    if (yTo &lt; 0) ofy = -size;
    
    var ins = collision_rectangle(x, y-size, (x+ofx)+(xTo*6), y+size, obj_solid, true, false);
    if (ins){ if (!ins.solid){ ins = false; } }
    if (!ins){
        x += xTo * spd;
    }
    
    var ins = collision_rectangle(x-size, y, x+size, (y-ofy)-(yTo*6), obj_solid, true, false);
    if (ins){ if (!ins.solid){ ins = false; } }
    if (!ins){
        y -= yTo * spd;
    }
}
</string>
